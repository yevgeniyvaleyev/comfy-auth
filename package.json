{
  "name": "comfy-auth",
  "version": "0.0.1",
  "author": "Yevgeniy Valeyev",
  "engines": {
    "node": "14.17.1",
    "npm": "6.14.13"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run build:ssr",
    "build:prod": "ng build --prod",
    "heroku-postbuild": "npm run build:prod",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:prod": "ng test --browsers=ChromeHeadless --watch=false --code-coverage",
    "dev:ssr": "ng run comfy-auth:serve-ssr",
    "serve:ssr": "node dist/comfy-auth/server/main.js",
    "build:ssr": "ng build && ng run comfy-auth:server:production",
    "prerender": "ng run comfy-auth:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.0.5",
    "@angular/cdk": "^12.2.8",
    "@angular/common": "~12.0.5",
    "@angular/compiler": "~12.0.5",
    "@angular/core": "~12.0.5",
    "@angular/forms": "~12.0.5",
    "@angular/material": "^12.2.8",
    "@angular/platform-browser": "~12.0.5",
    "@angular/platform-browser-dynamic": "~12.0.5",
    "@angular/platform-server": "~12.0.5",
    "@angular/router": "~12.0.5",
    "@nguniversal/express-engine": "^12.0.2",
    "bootstrap": "^5.1.2",
    "express": "^4.17.1",
    "localstorage-polyfill": "^1.0.1",
    "rxjs": "~6.6.0",
    "tslib": "^2.1.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.0.5",
    "@angular/cli": "~12.0.5",
    "@angular/compiler-cli": "~12.0.5",
    "@nguniversal/builders": "^12.0.2",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "jasmine-core": "~3.7.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "typescript": "~4.2.3"
  }
}
